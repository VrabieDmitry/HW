print('программа 2')
n = input()
print(len(n))
print(n[0])
print(n[-1])
print(n[::-1])
print(n[::2])
print(n[1::2])

print('программа 3')
n = input()
s = 1
if n.isdigit():
for i in n:
print(int(i) * 10)
elif n.find("q") == 1:
print(n.count("q"))

else:
r = 0
for i in n:
if i == '!' or i == '?' or i == '#' or i == '/' or i == '*' or i == '&':
print(r)
r += 1

print('программа 4')
while True:
s = input("Задайте строку: ")
if s.upper() == "STOP":
break
if s.isalpha():
print("Буквенная строка")
elif s.isdigit():
print("Цифровая строка")
elif s.isalnum():
print("Смешанная строка")


дз6
print('программа 1')
a = int(input('Задайте размер списка:'))
b = int(input('Задайте начало диапозона:'))
c = int(input('Задайте конец диапозона:'))
n = list()
for i in range(1, a+1):
q = randint(b, c)
n.append(q)
print(n)
print(len(n))
print(max(n))
print(min(n))
w = sorted(n)
print(w)
print(w[::-1])

print('программа 2')
a = int(input())
n = list()
for i in range(1, a+1):
q = randint(0, 1)
n.append(q)
print(n)
print(n.count(0))
print(n.count(1))
s = list()
for j in range(len(n)):
if n[j] == 0:
s.append(j)
print(s)

print('программа 3')
a = int(input('введите размер списка '))
n = list()
for i in range(1, a+1):
q = randint(0, 9)
n.append(q)
print(n)
b = int(input('введите число которое нужно удалить '))
for j in range(len(n)-1, -1, -1):
if n[j] == b:
n.pop(j)
print(n)
