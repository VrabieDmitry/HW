print('введите число')
d = int(input())
a = str(d % 10)
b = str(d//10 % 10)
c = str(d//100 % 10)
if a > b and a > c:
print(a)
elif b > a and b > c:
print(b)
else:
print(c)

print("введите число")
a = int(input())
b = int(input())
c = int(input())
d = int(input())
count = 0
g = 0
if a % 2 > 0:
count += 1
g += a
if b % 2 > 0:
count += 1
g += b
if c % 2 > 0:
count += 1
g += c
if d % 2 > 0:
count += 1
g += d
print(count, 'количество ничетных чисел')
print(g, "сумма ничётных чисел")

