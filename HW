print("введите число")
d = int(input())
a = str(d % 10)
b = str(d//10 % 10)
c = str(d//100 % 10)
print(a+b+c)
print(a+c+b)
print(b+a+c)
print(b+c+a)
print(c+a+b)
print(c+b+a)

print('введите время готовки')
a = int(input())
print('введите время уборки')
b = int(input())
print('введите время стирки')
c = int(input())
d=600
e=d+a+b+c
i=e%60
t=e//60
if i<10:
i='0'+str(i)
while t > 24:
t = t-24
if t < 10:
t = "0" + str(t)
print(t, ':', i)

